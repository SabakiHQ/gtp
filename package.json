{
  "name": "@sabaki/gtp",
  "version": "3.0.0",
  "description": "A Node.js module for handling GTP engines.",
  "main": "./src/main.js",
  "scripts": {
    "test": "cd tests && tap *.test.js",
    "format": "prettier **/*.{js,md} --write",
    "format-watch": "onchange \"**/*.{js,md}\" -- prettier --write {{changed}}"
  },
  "repository": {

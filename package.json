    "url": "git+https://github.com/SabakiHQ/gtp.git"
  },
  "keywords": [
    "gtp",
    "go",
    "weiqi",
    "baduk",
    "engine",
    "boardgame",
    "ai"
  ],
  "author": "Yichuan Shen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SabakiHQ/gtp/issues"
  },
  "homepage": "https://github.com/SabakiHQ/gtp",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "proseWrap": "always"
  },
  "devDependencies": {
    "onchange": "^6.1.0",
    "prettier": "1.19.1",
    "tap": "^14.10.6"
